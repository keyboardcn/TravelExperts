<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @*<link href="~/Content/bootstrap.css" rel="stylesheet" />
        <script src="~/Scripts/bootstrap.min.js"></script>*@
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
    <script src="Assets/js/core/bootstrap-material-design.min.js"></script>
    <link href="~/CSS/material-kit.css" rel="stylesheet" />
    <link href="~/CSS/material-kit.min.css" rel="stylesheet" />
    <script src="Assets/js/core/jquery.min.js"></script>
    <script src="~/Assets/js/core/popper.min.js"></script>

    <script src="Assets/js/plugins/moment.min.js"></script>
    @Styles.Render("~/CSS/material-kit.css")
    @Styles.Render("~/CSS/material-kit.min.css")
    @Styles.Render("https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons")
    @Styles.Render("https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css")
    @Scripts.Render("Assets/js/core/jquery.min.js")
    @Scripts.Render("~/Assets/js/core/popper.min.js")
    @Scripts.Render("Assets/js/core/bootstrap-material-design.min.js")
    @Scripts.Render("Assets/js/plugins/moment.min.js")
</head>
<body >
    <div class="navbar navbar-expand-lg bg-white navbar-fixed-top mx-sm-0 my-sm-0">
        <div class="container">
            <div class="navbar-header">
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                    <span class="navbar-toggler-icon"></span>
                    <span class="navbar-toggler-icon"></span>               
                </button>
                <a href="http://localhost:62385/Index.aspx" class="navbar-brand brand">TravelExperts</a>
            </div>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="nav navbar-nav">
                    @*<li class="nav-item">@Html.ActionLink("Home", "Index", "Home", new { @class = "nav-link" })</li>*@
                    <li class="nav-item">@Html.ActionLink("Create", "Create", "Home", new { @class = "nav-link" })</li>
                    <li class="nav-item">@Html.ActionLink("Login", "Login", "Home", new { @class = "nav-link" })</li>
                    @*<li class="nav-item">@Html.ActionLink("Customers", "Customers", "Home", new { @class = "nav-link" })</li>*@
                </ul>
            </div>
        </div>
    </div>
    @*<div class="container body-content">*@
    @*<div>*@

    @RenderBody()
    <footer class="footer">
        <div class="container">
            <div class="copyright float-right">
                &copy;
                <script>
                    document.write(new Date().getFullYear())
                </script>, made with <i class="material-icons">favorite</i> by
                Bold IT Solutions
            </div>
        </div>
    </footer>
    @*</div>*@
    <!-- Place this tag in your head or just before your close body tag. -->
    <script async defer src="https://buttons.github.io/buttons.js"></script>
    <!-- Control Center for Material Kit: parallax effects, scripts for the example pages etc -->
    <script src="assets/js/material-kit.js?v=2.0.5" type="text/javascript"></script>
    @Styles.Render("~/CSS/material-kit.css")
    @Styles.Render("~/CSS/material-kit.min.css")
    @Styles.Render("https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons")
    @Styles.Render("https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css")
    @Scripts.Render("Assets/js/core/jquery.min.js")
    @Scripts.Render("~/Assets/js/core/popper.min.js")
    @Scripts.Render("Assets/js/core/bootstrap-material-design.min.js")
    @Scripts.Render("Assets/js/plugins/moment.min.js")
    @RenderSection("scripts", required: false)
</body>
</html>
